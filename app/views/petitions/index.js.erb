$loadedData = $("<%= escape_javascript(render partial: 'list', layout: false) %>");

$('.petitions-overview-container .clearfix').before($loadedData);

$loadedData.find('.chart-canvas').each(function(index, elem){ initChart(elem); });

$('a.navigation-loadmore:first').attr('href', '<%= raw(path_to_next_page(@petitions, params: { sorting: @sorting })) %>');
